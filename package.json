{
  "name": "@segmentio/tsub",
  "version": "0.0.1",
  "description": "Tsub for JS",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc",
    "lint": "tslint src/**/*.ts"
  },
  "repository": "git+https://github.com/segmentio/tsub-js.git",
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/segmentio/tsub-js/issues"
  },
  "homepage": "https://github.com/segmentio/tsub-js#readme",
  "dependencies": {
    "@segment/fql": "^1.6.1",
    "js-md5": "^0.7.3",
    "lodash.get": "^4.4.2",
    "math-float64-ldexp": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "yarn lint",
        "prettier --write",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "examples/**/generated/**",
      "**/__snapshots__/**"
    ]
  }
}
