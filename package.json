{
  "name": "@segmentio/tsub",
  "version": "0.0.2",
  "description": "Tsub for JS",
  "main": "dist/src/index.js",
  "browser": "dist/src/index.js",
  "bin": "dist/src/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc",
    "lint": "tslint src/**/*.ts",
    "lint_test": "tslint src/**/*.test.ts"
  },
  "repository": "git+https://github.com/segmentio/tsub-js.git",
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/segmentio/tsub-js/issues"
  },
  "homepage": "https://github.com/segmentio/tsub-js#readme",
  "dependencies": {
    "@segment/fql": "^1.6.1",
    "dlv": "^1.1.3",
    "dset": "^2.0.1",
    "math-float64-ldexp": "^1.0.1",
    "tiny-hashes": "^1.0.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "size-limit": "^4.9.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "size-limit": [
    {
      "path": "dist/src/index.js",
      "limit": "10 KB"
    }
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "prettier --write",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "examples/**/generated/**",
      "**/__snapshots__/**"
    ]
  }
}
